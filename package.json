{
  "name": "cr2-smart-table",
  "version": "2.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build demo --aot --prod",
    "build:lib": "ng build cr2-smart-table  --configuration production",
    "build:ci": "npm run build:demo && npm run build:lib",
    "docs:gh-pages": "npm run build:demo -- --base-href '/cr2-smart-table/' && ngh --dir ./dist/demo",
    "test": "ng test",
    "release": "npm run build:lib && npm run publish",
    "publish": "npm publish --access=public ./dist/cr2-smart-table",
    "changelog": "standard-changelog -p angular",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/animations": "~17.3.3",
    "@angular/common": "~17.3.3",
    "@angular/compiler": "~17.3.3",
    "@angular/core": "~17.3.3",
    "@angular/forms": "~17.3.3",
    "@angular/platform-browser": "~17.3.3",
    "@angular/platform-browser-dynamic": "~17.3.3",
    "@angular/router": "~17.3.3",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.10",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular/cli": "~17.3.3",
