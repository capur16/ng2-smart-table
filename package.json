    "angular-cli-ghpages": "^2.0.0-beta.2",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^15.2.2",
    "protractor": "^7.0.0",
    "standard-changelog": "^2.0.11",
